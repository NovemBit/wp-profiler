{
  "name": "novembit/wp-profiler",
  "description": "WordPress profiling plugin.",
  "keywords": [
    "logger",
    "debugger"
  ],
  "license": [
    "GPL-3.0-or-later"
  ],
  "minimum-stability": "stable",
  "type": "project",
  "require": {
    "php": ">=5.6",
    "yiisoft/yii2": "~2.0.14",
    "ext-pdo": "*",
    "wp-coding-standards/wpcs": "2.1.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "extra": {
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
